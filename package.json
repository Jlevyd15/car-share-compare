    "file-loader": "^1.1.5",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "jest": "^22.4.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.2",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "verbose": true
  },
  "babel": {
    "plugins": [
      "transform-class-properties"
