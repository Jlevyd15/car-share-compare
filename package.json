  "bugs": {
    "url": "https://github.com/Jlevyd15/car-share-compare/issues"
  },
  "homepage": "https://github.com/Jlevyd15/car-share-compare#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "~1.13.2",
    "clean-webpack-plugin": "^0.1.17",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "mongoose": "^4.5.5",
    "morgan": "~1.6.1",
    "nodemailer": "^2.6.4",
    "react": "^16.3.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.19.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.0.0"
