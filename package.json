{
  "name": "car-share-compare",
  "version": "1.0.0",
  "description": "compare car share services",
  "scripts": {
    "server": "cross-env NODE_ENV=development nodemon ./server/app.js",
    "client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "start": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js\" \"cross-env NODE_ENV=development nodemon ./server/app.js\"",
    "build": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.config.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jlevyd15/car-share-compare.git"
  },
  "author": "Jeremy Levy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jlevyd15/car-share-compare/issues"
  },
  "homepage": "https://github.com/Jlevyd15/car-share-compare#readme",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^8.0.0",
    "body-parser": "~1.13.2",
    "browserslist": "^4.2.1",
    "clean-webpack-plugin": "^0.1.17",
    "connected-react-router": "^4.3.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "express": "^4.13.3",
    "express-subdomain": "^1.0.5",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "mongoose": "^4.5.5",
    "morgan": "~1.6.1",
    "nodemailer": "^2.6.4",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.19.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-async-to-generator": "7.1.0",
    "babel-eslint": "^8.2.3",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^4.0.0-beta.1",
    "jest": "^22.4.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.2",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "verbose": true
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-async-to-generator"
    ],
    "presets": [
      "@babel/preset-react",
      "@babel/env"
    ]
  },
	"browserslist": "> 0.25%, not dead",
	"nodemonConfig": {
		"ignore": [
			"frontend/*",
			"docs/*",
			"_docs/*",
			"node_modules/*"
		],
		"delay": "2500"
	}
}
