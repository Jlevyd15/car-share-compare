{
  "name": "car-share-compare",
  "version": "1.0.0",
  "description": "compare car share services",
  "scripts": {
    "start-server": "cross-env NODE_ENV=development nodemon ./server/app.js",
    "start-frontend": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "start": "concurrently \"webpack-dev-server --config build/webpack.config.js\" \"cross-env NODE_ENV=development nodemon ./server/app.js\"",
    "build": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.config.js",
    "test": "jest --watch"
  },
